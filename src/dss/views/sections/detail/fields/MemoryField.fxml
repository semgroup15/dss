<?import dss.views.sections.detail.fields.base.GroupLabel?>
<?import dss.views.sections.detail.fields.base.UnitTextField?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<fx:root
        type="javafx.scene.layout.BorderPane"
        xmlns:fx="http://javafx.com/fxml">

    <left>
        <GroupLabel text="Memory"/>
    </left>

    <center>
        <HBox>
            <UnitTextField
                    fx:id="ram"
                    valuePromptText="RAM"
                    unit="MB"/>

            <UnitTextField
                    fx:id="internal"
                    valuePromptText="Internal"
                    unit="MB"/>
        </HBox>
    </center>
</fx:root>
